{
  "name": "frontend",
  "version": "1.0.0",
  "author": "https://github.com/VadAbr",
  "main": "src/index.tsx",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "start:dev": "webpack serve --config webpack/dev.config.js",
    "start:prod": "webpack serve --config webpack/prod.config.js",
    "build:dev": "webpack --config webpack/dev.config.js",
    "build:prod": "webpack --config webpack/prod.config.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint src -c eslint.config.mjs --quiet --color --cache --cache-location .cache/.eslintcache",
    "lint:style": "stylelint \"**/*.css\" --cache --cache-location .cache/.stylelintcache",
    "lint:fix": "npm run lint:code:fix && npm run lint:style:fix",
    "lint:code:fix": "eslint src -c eslint.config.mjs --quiet --fix --cache --cache-location .cache/.eslintcache",
    "lint:style:fix": "stylelint \"**/*.css\" --fix --cache --cache-location .cache/.stylelintcache",
    "typecheck": "tsc --pretty --noEmit",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react-dom": "^19.1.3",
    "@types/react-redux": "^7.1.34",
    "css-loader": "^6.8.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "html-webpack-plugin": "^5.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-clean-order": "^5.4.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.20.0",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@consta/icons": "^1.1.0",
    "@consta/uikit": "~5.15.0",
    "@reduxjs/toolkit": "^2.7.0",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.23.0"
  }
}
